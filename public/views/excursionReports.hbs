<h3>{{title}}
    <button class="button btn-secondary" id="addButton" onclick="createExcursionReport()" style="margin-left: 10px;">
        Добавить
    </button>
</h3>
<div class="table-responsive">
    <table class="table table-dark">
        <thead id="thead1"></thead>
        <tbody id="tbody1"></tbody>
    </table>
</div>

<form class="decor">
    <div class="form-left-decoration"></div>
    <div class="form-right-decoration"></div>
    <div class="circle"></div>
    <div class="form-inner">
        <h3>Добавление отчета об экскурсии</h3>
        <input id='inpNum' placeholder="Порядковый номер экскурсии">
        <textarea id='inpInf' placeholder="Информация о посетителях" rows="3"></textarea>
        <input id='inpNameExcurs' placeholder="Название экскурсии">
        <input id='inpDateSpend' placeholder="Дата проведения">
        <input id='inpTabNum' placeholder="Табельный номер">
        <input id='inpBtnAdd' type="submit" value="Добавить">
    </div>
    <button type="button" class="close " aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</form>


<a href="#" onclick="return up()" id="toTopBtn"
   class="cd-top text-replace js-cd-top cd-top--is-visible cd-top--fade-out" data-abc="true">Top</a>
<script src="excursionReports.js"></script>