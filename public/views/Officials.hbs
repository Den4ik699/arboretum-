<h3>{{title}}
    <button class="button btn-secondary" id="addButton" onclick="createOfficial()" style="margin-left: 10px;">Добавить
    </button>
</h3>
<div class="table-responsive">
    <table class="table table-striped table-dark">
        <thead id="thead1"></thead>
        <tbody id="tbody1"></tbody>
    </table>
</div>


<form class="decor">
    <div class="form-left-decoration"></div>
    <div class="form-right-decoration"></div>
    <div class="circle"></div>
    <div class="form-inner">
        <h3>Добавление должностного лица</h3>
        <input id='inpTabNum' type="tabNum" placeholder="Табельный номер">
        <input id='inpfio' type="fio" placeholder="ФИО">
        <input id='inprank' type="rank" placeholder="Административная должность">
        <input id='inprespons' type="rank" placeholder="Должностные обязанности">
        <input id='inpBtnAdd' type="submit" value="Добавить">
    </div>
    <button type="button" class="close " aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</form>


<a href="#" onclick="return up()" id="toTopBtn"
   class="cd-top text-replace js-cd-top cd-top--is-visible cd-top--fade-out" data-abc="true">Top</a>

<script src="Officials.js"></script>