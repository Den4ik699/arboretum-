<h3>{{title}}
    <button class="button btn-secondary" id="addButton" onclick="createRegion()" style="margin-left: 10px;">Добавить
    </button>
</h3>
<div class="table-responsive">
    <table class="table table-striped table-dark">
        <thead id="thead1"></thead>
        <tbody id="tbody1"></tbody>
    </table>
</div>


<form class="decor">
    <div class="form-left-decoration"></div>
    <div class="form-right-decoration"></div>
    <div class="circle"></div>
    <div class="form-inner">
        <h3>Добавление региона</h3>
        <input required id='inpNum' type="number" placeholder="Номер региона">
        <input required id='inpName' type="text" placeholder="Название региона">
        <input required id='inpLocation' type="text" placeholder="Положение на схеме">
        <input required id='inpInfo' type="text" placeholder="Краткая информация">
        <input required id='inpBtnAdd' type="submit" value="Добавить">
    </div>
    <button type="button" class="close " aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</form>


<a href="#" onclick="return up()" id="toTopBtn"
   class="cd-top text-replace js-cd-top cd-top--is-visible cd-top--fade-out" data-abc="true">Top</a>

<script src="regionOfDendropark.js"></script>